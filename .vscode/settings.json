{
    "cucumberautocomplete.steps": [
        "./cypress/integration/step_definitions/*.ts"
    ],
    "cucumberautocomplete.syncfeatures": "./cypress/integration/features/*feature",
    "cucumberautocomplete.strictGherkinCompletion": false,
    "editor.quickSuggestions": {
        "strings": true,
    },
    "cucumber-quick": {
        "tool": "cypress",
        "script": "cypress run  --spec 'cypress/integration/features/**/*.feature' --browser chrome"
        //"cypress run --spec 'cypress/integration/features/**/*.feature'"
    }
  }